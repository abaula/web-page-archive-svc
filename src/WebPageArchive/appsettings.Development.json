{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "http://+:8000",
        "Protocols": "Http2"
      }
    }
  },
  "PageWaitSettings": {
    "UseDefaultScript": true,
    "TimeoutMs": 15000
  }
}
